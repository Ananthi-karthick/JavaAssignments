import java.util.Scanner;
public class task1 
{
public static void main(String[] args) {

        int n;
        Scanner in=new Scanner(System.in);
        System.out.println("Enter the place you need to know:");
        n=in.nextInt();
        in.close();
        int count=0;
        long x=1;
       while(count<=100)
        {
        	x=x*2;
            count++;
            if(count==n)
        	{
        		 System.out.println(x);
        	}
        	
        }
          
        
	}

}
